        ANL     0xf4,#0xdf
//  251   HAL_KEY_SW_9_DIR &= ~(HAL_KEY_SW_9_BIT);    /* Set pin direction to Input */
        ANL     0xfe,#0xdf
//  252 
//  253 
//  254   
//  255 
